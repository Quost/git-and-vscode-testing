ner.scan().then(barcodeData => {\n      console.log('Barcode data', barcodeData);\n      this.data = barcodeData;\n    }).catch(err => {\n      console.log('Error', err);\n    });\n  }\n}\n"],"sourceRoot":"webpack:///"}